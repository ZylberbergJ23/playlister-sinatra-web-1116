
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <% if flash.has?(:message) %>
      <%= flash[:message] %>
    <% end %>
    <%=@song.name%>
    <%=@song.artist.name%>
    <a href="/artists/<%="#{@artist.slug}"%>">Link</a>
    <%@song.genres.each do |genre|%>
      <%= genre.name %>
      <a href="/genres/<%="#{genre.slug}"%>">Link</a>
    <% end %>
  </body>
</html>
